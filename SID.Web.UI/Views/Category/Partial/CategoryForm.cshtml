
@model SID.Web.UI.Models.VM.CategoryVM

@Html.HiddenFor(q => q.ID)

<div class="form-group">
    <label>Ad:</label>
    @Html.TextBoxFor(q => q.Name, new { @class = "form-control" })
    <small id="emailHelp" class="form-text text-muted">Kategori adıyla ilgili not</small>
</div>
<div class="form-group">
    <label>Açıklama</label>
    @Html.TextAreaFor(q => q.Description, new { @class = "form-control" })
</div>
