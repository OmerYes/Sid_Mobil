
@model SID.Web.UI.Models.VM.BrandVM

@Html.HiddenFor(q => q.ID)

    <div class="form-group">
      
        <label>Marka Adı</label>
        @Html.TextBoxFor(q => q.Name, new { @class = "form-control" })
    </div>
<div class="form-group">
    <label>Logo</label>
    <div class="custom-file">
        <input type="file" class="custom-file-input" id="files" name="logo" >
        <label class="custom-file-label" for="files">Marka Logosunu Seçiniz</label>
    </div>
</div>

